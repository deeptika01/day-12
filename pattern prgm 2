#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int rows;
    scanf("%d", &rows);
    for (int a=1;a<=rows;a++)
    {
        for (int b=1;b<=a;b++)
        {
            printf("*");
        }
        printf("\n");
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
