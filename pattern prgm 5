int main() {
    int n;
    scanf("%d", &n);
    if (n <= 0) {
        printf("Input must be greater than 0\n");
        return 1;
    }


    for (int i = 1; i <= n; i++) {
        int num;

        if (i % 2 == 1) {
            num = 1;
        } else {
            num = 2;
        }

        for (int j = 0; j < i; j++) {
            printf("%d", num);
            num += 2;
        }
        printf("\n"); 
    }
    return 0;
}
